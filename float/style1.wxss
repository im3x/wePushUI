.float {
  width: 100rpx;
  height: 100rpx;
  position: fixed;
  right: 40rpx;
  bottom: 100rpx;
  background-color: #EEE;
  border-radius: 100rpx;
  overflow: hidden;
  z-index: 9;
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow: 0 5px 10px -5px #1EA6B7;
}
.float > image {
  width: 100%;
  height: 100%;
}
.float-hover {
  opacity: 0.6;
}